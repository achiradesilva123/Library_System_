<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="284.0" prefWidth="334.0" stylesheets="@../Style/Addbook.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.LS.Controller.SettingController">
   <children>
      <VBox prefHeight="243.0" prefWidth="334.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="txtNofWithoutFine" labelFloat="true" promptText="Number of day Without fine">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="30.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtFineperDay" labelFloat="true" promptText="Fine per Day">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="txtuserName" labelFloat="true" promptText="User name">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXPasswordField fx:id="txtPassword" labelFloat="true" promptText="Password">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXPasswordField>
         </children>
      </VBox>
      <HBox layoutY="241.0" prefHeight="44.0" prefWidth="334.0">
         <children>
            <JFXButton fx:id="btSave" onAction="#SetSetting" prefHeight="45.0" prefWidth="171.0" styleClass="bablu" stylesheets="@../Style/Addbook.css" text="Save" />
            <JFXButton fx:id="btCancel" onAction="#CancelAction" prefHeight="45.0" prefWidth="171.0" styleClass="bablu" stylesheets="@../Style/Addbook.css" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
